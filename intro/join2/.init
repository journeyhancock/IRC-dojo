#!/opt/pwn.college/python

import subprocess
import pathlib

dir = pathlib.Path(__file__).parent

with open(dir / "thisworks.txt", "w") as f:
    f.write("flag \n")

commands = ["cd sic-1.3", "make", "make install"]

for command in commands:
    try:
        subprocess.run(command, shell=True, check=True)
        with open(dir / "yes.txt", "w") as f:
            f.write(f"Successfully executed: {command}")

    except subprocess.CalledProcessError as e:
        with open(dir / "no.txt", "w") as f:
            f.write(f"Error occurred while executing: {command}")